\f0\b \cf2 Number of Schools in Each Zip Code\

\f1\b0 SELECT \
public_hs_data.zip_code,\
COUNT(public_hs_data.school_id) AS "Number of Schools in Zip Code"\
FROM public_hs_data\
GROUP BY public_hs_data.zip_code\
ORDER BY 2 DESC;\
\

\f0\b Number of Schools in Each State\

\f1\b0 SELECT \
public_hs_data.state_code AS "State",\
COUNT(public_hs_data.school_id) AS "Number of Schools in Zip Code"\
FROM public_hs_data\
GROUP BY 1\
ORDER BY 2 DESC;\
