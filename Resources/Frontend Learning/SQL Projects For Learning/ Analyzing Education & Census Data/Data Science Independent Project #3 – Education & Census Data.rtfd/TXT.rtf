\fs30 \cf2 \kerning1\expnd0\expndtw0 \
SELECT \
school_name,\
locale_code,\
CASE \
	WHEN CAST(substr(locale_code, 1,1) AS INTEGER) = 1 THEN 'City'\
	WHEN CAST(substr(locale_code, 1,1) AS INTEGER) = 2 THEN 'Suburb'\
	WHEN CAST(substr(locale_code, 1,1) AS INTEGER) = 3 THEN 'Town'\
	WHEN CAST(substr(locale_code, 1,1) AS INTEGER) = 4 THEN 'Rural'\
	ELSE 'Undefined'\
END AS 'locale_text',\
CASE \
	WHEN CAST(substr(locale_code, 2,1) AS INTEGER) = 1 THEN 'Large'\
	WHEN CAST(substr(locale_code, 2,1) AS INTEGER) = 2 THEN 'Midsize'\
	WHEN CAST(substr(locale_code, 2,1) AS INTEGER) = 3 THEN 'Small'\
	ELSE 'Undefined'\
END AS 'locale_size'\
FROM public_hs_data;\
\
